<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="dotnet_maui_coin_converter.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:dotnet_maui_coin_converter"
    xmlns:localview="clr-namespace:dotnet_maui_coin_converter.View"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Home" FlyoutIcon="{StaticResource IconOne}">
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="OnePage" />
    </FlyoutItem>
   
    <FlyoutItem Title="Coin Converter" FlyoutIcon="{StaticResource IconTwo}">
        <ShellContent
            Title="Coin Converter"
            ContentTemplate="{DataTemplate localview:CoinConverterSLPage}"
            Route="TwoPage" />
    </FlyoutItem>

</Shell>
